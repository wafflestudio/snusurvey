<div class="table">
	<table class="listing" cellpadding="0" cellspacing="0">
		<tr>
							<th>content</th>
							<th>question_id</th>
							<th>reply_ids</th>
					</tr>
		<% @examples.each do |example| -%>
			<tr>
									<td>
													<%= example.content %>
											</td>
									<td>
													<%= example.question_id %>
											</td>
									<td>
													<%= example.reply_ids %>
											</td>
							</tr>
    <% end %>
		<% if @examples.empty? %>
      <tr>
				<td colspan="3">No records</td>
      </tr>
    <% end %>
  </table>
</div>

<div class="actions">
	<a href="<%= new_admin_example_path %>" class="action add">New Example</a>
</div>
