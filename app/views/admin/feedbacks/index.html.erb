<div class="table">
	<table class="listing" cellpadding="0" cellspacing="0">
		<tr>
							<th>created_at</th>
							<th>content</th>
							<th>password</th>
							<th>users_id</th>
					</tr>
		<% @feedbacks.each do |feedback| -%>
			<tr>
									<td>
													<%= feedback.created_at %>
											</td>
									<td>
													<%= feedback.content %>
											</td>
									<td>
													<%= feedback.password %>
											</td>
									<td>
													<%= feedback.users_id %>
											</td>
							</tr>
    <% end %>
		<% if @feedbacks.empty? %>
      <tr>
				<td colspan="4">No records</td>
      </tr>
    <% end %>
  </table>
</div>

<div class="actions">
	<a href="<%= new_admin_feedback_path %>" class="action add">New Feedback</a>
</div>
