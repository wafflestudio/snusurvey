<div class="table">
	<table class="listing" cellpadding="0" cellspacing="0">
		<tr>
							<th>created_at</th>
							<th>updated_at</th>
							<th>completed</th>
							<th>user_id</th>
							<th>survey_id</th>
					</tr>
		<% @papers.each do |paper| -%>
			<tr>
									<td>
													<%= paper.created_at %>
											</td>
									<td>
													<%= paper.updated_at %>
											</td>
									<td>
													<%= paper.completed %>
											</td>
									<td>
													<%= paper.user_id %>
											</td>
									<td>
													<%= paper.survey_id %>
											</td>
							</tr>
    <% end %>
		<% if @papers.empty? %>
      <tr>
				<td colspan="5">No records</td>
      </tr>
    <% end %>
  </table>
</div>

<div class="actions">
	<a href="<%= new_admin_paper_path %>" class="action add">New Paper</a>
</div>
