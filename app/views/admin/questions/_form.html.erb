
<%= form_for [:admin, @question] do |f| %>
  <div class="table">
    <table class="listing form" cellpadding="0" cellspacing="0">
			<th class="full" colspan="2">Adding a new Question</th>
							<tr>
					<td><strong>title</strong></td>
					<td>
						<%= f.text_field :title%>
					</td>
				</tr>
							<tr>
					<td><strong>description</strong></td>
					<td>
						<%= f.text_field :description%>
					</td>
				</tr>
							<tr>
					<td><strong>question_template_id</strong></td>
					<td>
						<%= f.text_field :question_template_id%>
					</td>
				</tr>
							<tr>
					<td><strong>survey_id</strong></td>
					<td>
						<%= f.text_field :survey_id%>
					</td>
				</tr>
				
    </table>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end -%>
