			<th class="full" colspan="2">Adding a new Reply</th>
							<tr>
					<td><strong>created_at</strong></td>
					<td>
						<%= f.text_field :created_at%>
					</td>
				</tr>
							<tr>
					<td><strong>updated_at</strong></td>
					<td>
						<%= f.text_field :updated_at%>
					</td>
				</tr>
							<tr>
					<td><strong>question_id</strong></td>
					<td>
						<%= f.text_field :question_id%>
					</td>
				</tr>
							<tr>
					<td><strong>paper_id</strong></td>
					<td>
						<%= f.text_field :paper_id%>
					</td>
				</tr>
							<tr>
					<td><strong>example_ids</strong></td>
