<div class="table">
	<table class="listing" cellpadding="0" cellspacing="0">
		<tr>
							<th>created_at</th>
							<th>updated_at</th>
							<th>question_id</th>
							<th>paper_id</th>
							<th>example_ids</th>
					</tr>
		<% @replies.each do |reply| -%>
			<tr>
									<td>
													<%= reply.created_at %>
											</td>
									<td>
													<%= reply.updated_at %>
											</td>
									<td>
													<%= reply.question_id %>
											</td>
									<td>
													<%= reply.paper_id %>
											</td>
									<td>
													<%= reply.example_ids %>
											</td>
							</tr>
    <% end %>
		<% if @replies.empty? %>
      <tr>
				<td colspan="5">No records</td>
      </tr>
    <% end %>
  </table>
</div>

<div class="actions">
	<a href="<%= new_admin_reply_path %>" class="action add">New Reply</a>
</div>
