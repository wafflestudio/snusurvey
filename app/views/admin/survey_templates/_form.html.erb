
<%= form_for [:admin, @survey_template] do |f| %>
  <div class="table">
    <table class="listing form" cellpadding="0" cellspacing="0">
			<th class="full" colspan="2">Adding a new SurveyTemplate</th>
							<tr>
					<td><strong>created_at</strong></td>
					<td>
						<%= f.text_field :created_at%>
					</td>
				</tr>
							<tr>
					<td><strong>updated_at</strong></td>
					<td>
						<%= f.text_field :updated_at%>
					</td>
				</tr>
							<tr>
					<td><strong>title</strong></td>
					<td>
						<%= f.text_field :title%>
					</td>
				</tr>
							<tr>
					<td><strong>description</strong></td>
					<td>
						<%= f.text_field :description%>
					</td>
				</tr>
							<tr>
					<td><strong>public</strong></td>
					<td>
						<%= f.text_field :public%>
					</td>
				</tr>
							<tr>
					<td><strong>perpage</strong></td>
					<td>
						<%= f.text_field :perpage%>
					</td>
				</tr>
				
    </table>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end -%>
