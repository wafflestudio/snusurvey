						<%= f.text_field :board_id%>
					</td>
				</tr>
							<tr>
					<td><strong>like_user_ids</strong></td>
					<td>
						<%= f.text_field :like_user_ids%>
					</td>
				</tr>
							<tr>
					<td><strong>dislike_user_ids</strong></td>
					<td>
						<%= f.text_field :dislike_user_ids%>
					</td>
				</tr>
				
    </table>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
