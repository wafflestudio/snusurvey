<div class="table">
	<table class="listing" cellpadding="0" cellspacing="0">
		<tr>
							<th>created_at</th>
							<th>updated_at</th>
							<th>title</th>
							<th>description</th>
							<th>template_id</th>
							<th>result_template_id</th>
							<th>admin_id</th>
					</tr>
		<% @surveys.each do |survey| -%>
			<tr>
									<td>
													<%= survey.created_at %>
											</td>
									<td>
													<%= survey.updated_at %>
											</td>
									<td>
													<%= link_to "#{survey.title}", admin_survey_path(survey) %>
											</td>
									<td>
													<%= survey.description %>
											</td>
									<td>
											</td>
									<td>
													<%= survey.result_template_id %>
											</td>
									<td>
													<%= survey.admin_id %>
											</td>
							</tr>
    <% end %>
		<% if @surveys.empty? %>
      <tr>
				<td colspan="7">No records</td>
      </tr>
    <% end %>
  </table>
</div>

<div class="actions">
	<a href="<%= new_admin_survey_path %>" class="action add">New Survey</a>
</div>
