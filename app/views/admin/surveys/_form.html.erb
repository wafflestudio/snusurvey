
<%= form_for [:admin, @survey] do |f| %>
  <div class="table">
    <table class="listing form" cellpadding="0" cellspacing="0">
			<th class="full" colspan="2">Adding a new Survey</th>
							<tr>
					<td><strong>created_at</strong></td>
					<td>
						<%= f.text_field :created_at%>
					</td>
				</tr>
							<tr>
					<td><strong>updated_at</strong></td>
					<td>
						<%= f.text_field :updated_at%>
					</td>
				</tr>
							<tr>
					<td><strong>title</strong></td>
					<td>
						<%= f.text_field :title%>
					</td>
				</tr>
							<tr>
					<td><strong>description</strong></td>
					<td>
						<%= f.text_field :description%>
					</td>
				</tr>
							<tr>
					<td><strong>template_id</strong></td>
					<td>
						<%= f.text_field :template_id%>
					</td>
				</tr>
							<tr>
					<td><strong>result_template_id</strong></td>
					<td>
						<%= f.text_field :result_template_id%>
					</td>
				</tr>
							<tr>
					<td><strong>admin_id</strong></td>
					<td>
						<%= f.text_field :admin_id%>
					</td>
				</tr>
				
    </table>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end -%>
